program PrjLogin;

uses
  Vcl.Forms,
  UntLogin in 'Login\UntLogin.pas' {Frmlogin},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Turquoise Gray');
  Application.CreateForm(TFrmlogin, Frmlogin);
  Application.Run;
end.
